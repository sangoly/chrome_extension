{
    "name": "open saved",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Will help you open saved page",
    "content_security_policy": "script-src 'self' http://127.0.0.1; object-src 'self'",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs"
    ]
}
